#include <stdio.h>
 
int main(void)
{
    int month, day, hour, min;
    char *name;
     
    month = 7;
    day = 27;
    hour = 9;
    min = 45;
    name = "kaiching";
     
    printf("\n\n今天是 %d 月 %d 日\n", month, day);
    printf("現在是上午 %d:%d\n", hour, min);
    printf("哈囉, %s！\n\n", name);
     
    return 0;
}
 
/* 《程式語言教學誌》的範例程式
